class Solution:
    def reverseVowels(self, s: str) -> str:
        res="";
        vowels=["a","e","i","o","u","A","E","I","O","U"];
        lastV=len(s);
        for i in range(0,len(s)):
            if s[i] not in vowels:
                res=res+s[i];  
            else :
                for j in reversed(range(0,lastV)):
                    if s[j] in vowels:
                        lastV=j;
                        res=res+s[j];
                        break;
        return res;
